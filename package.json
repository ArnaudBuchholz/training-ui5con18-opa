{
  "name": "training-uicon18-opa",
  "version": "0.1.0",
  "description": "Sample app to demonstrate UI5's OPA (UICon'18)",
  "private": true,
  "scripts": {
    "start": "ui5 serve -o index.html",
    "serve": "ui5 serve",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "start-server-and-test start http://localhost:8080 karma",
    "test": "npm run lint && rimraf coverage && start-server-and-test serve http://localhost:8080 karma-ci",
    "build": "rimraf dist && ui5 build --a"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.60.2",
    "@openui5/sap.ui.core": "^1.60.2",
    "@openui5/sap.ui.layout": "^1.60.2",
    "@openui5/themelib_sap_belize": "^1.60.2"
  },
  "devDependencies": {
    "@ui5/cli": "^0.2.7",
    "eslint": "^5.10.0",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-openui5": "^0.2.3",
    "karma-qunit": "^2.1.0",
    "qunitjs": "^2.4.1",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.7.11"
  }
}
