{
  "name": "training-ui5con18-opa",
  "version": "1.0.0",
  "description": "Sample app to demonstrate UI5's OPA (UICon'18)",
  "private": true,
  "scripts": {
    "clean": "rimraf dist & rimraf report & rimraf .nyc_output & rimraf coverage",
    "start": "ui5 serve -o index.html",
    "serve": "ui5 serve",
    "lint": "eslint demos webapp workshop",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "watch": "start-server-and-test start http://localhost:8080 karma",
    "test": "npm run lint && rimraf coverage && start-server-and-test serve http://localhost:8080 karma-ci",
    "build": "rimraf dist && ui5 build --a",
    "ui5con19": "start-server-and-test serve-qunit-mockserver http://localhost:8080 setup-ui5con19",
    "serve-qunit-mockserver": "ui5 serve -o test/unit/unitTests.qunit.html?module=MockServer",
    "setup-ui5con19": "node ./workshop/setup ui5con19-mockserver",
    "reserve": "reserve",
    "ui5-version": "node ui5-version"
  },
  "dependencies": {
    "@openui5/sap.m": "^1.120.6",
    "@openui5/sap.ui.core": "^1.120.6",
    "@openui5/sap.ui.layout": "^1.120.6",
    "@openui5/themelib_sap_belize": "^1.120.6"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.2.3",
    "@stryker-mutator/karma-runner": "^8.2.3",
    "@ui5/cli": "^3.9.1",
    "@ui5/middleware-code-coverage": "^1.1.1",
    "colors": "^1.4.0",
    "eslint": "^8.56.0",
    "gpf-js": "^1.0.0",
    "inquirer": "^9.2.14",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-ui5": "^3.0.4",
    "lodash": "^4.17.21",
    "mocha": "^10.3.0",
    "node-qunit": "^2.0.0",
    "qunit": "^2.20.0",
    "reserve": "^1.15.6",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "start-server-and-test": "^2.0.3"
  },
  "optionalDependencies": {
    "node-ui5": "^2.0.4"
  }
}
